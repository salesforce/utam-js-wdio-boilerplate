{
    "name": "utam-js-webdriverio-boilerplate",
    "version": "0.0.1",
    "private": true,
    "types": "build/index.d.ts",
    "main": "build/index.js",
    "scripts": {
        "postinstall": "yarn link -s && yarn link utam-js-webdriverio-boilerplate -s",
        "prepare": "yarn build",
        "test": "wdio",
