        "build:ts": "tsc -b tsconfig.json",
        "build:utam": "utam -c utam.config.js"
    },
    "devDependencies": {
        "@types/jasmine":"^3.6.3",
        "@wdio/cli": "^6.12.1",
        "@wdio/jasmine-framework": "^6.11.0",
        "@wdio/local-runner": "^6.12.1",
        "@wdio/spec-reporter": "^6.11.0",
        "chromedriver": "^88.0.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "wdio-chromedriver-service": "^6.0.4",
        "wdio-utam-service": "0.0.2",
        "dotenv": "^8.2.0"
    },
    "volta": {
        "node": "14.15.4",
        "yarn": "1.22.5"
    },
    "engines": {
        "node": ">=14.15.4 <15"
    }
}
